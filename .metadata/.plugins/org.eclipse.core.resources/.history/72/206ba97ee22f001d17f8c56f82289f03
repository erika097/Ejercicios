#include <stdio.h>
#include <stdlib.h>

int Restar1(int num1, int num2);


int main(void) {
	setbuf(stdout, NULL);

	int num1;
	int num2;

	printf("ingrese el primer numero: ");
	scanf("%d", &num1);
	printf("ingrese el segundo numero: ");
	scanf("%d", &num2);

	printf("la resta da: %d", Restar1(num1, num2));

	return EXIT_SUCCESS;
}

int Restar1(int num1, int num2)
{
	int resta;

	resta = num1 - num2;

	return resta;
}
