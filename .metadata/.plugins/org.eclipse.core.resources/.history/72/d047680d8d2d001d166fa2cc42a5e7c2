#include <stdio.h>
#include <stdlib.h>

int main(void) {
	setbuf(stdout, NULL);

	int num1;
	int num2;
	int suma;

	printf("ingrese un numero entre -50 y 100: ");
	scanf("%d", &num1);
	printf("ingrese otro numero entre -50 y 100: ");
	scanf("%d", &num2);
	suma = num1 + num2;
	printf("la suma de los numeros ingresados es: %d", &suma);

	return EXIT_SUCCESS;
}
