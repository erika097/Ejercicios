#include <stdio.h>
#include <stdlib.h>

int sumarRestar(int a, int b, char c)
{
	int suma;
	int resta;
	int resultado;

	if(c == 's')
	{
		suma = a + b;
		resultado = suma;
	}
	else
	{
		resta = a - b;
		resultado = resta;
	}

	return resultado;
}
