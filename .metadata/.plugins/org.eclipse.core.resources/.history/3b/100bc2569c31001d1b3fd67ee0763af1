#include <stdio.h>
#include <stdlib.h>

int pedirNumero();

int main(void) {
	setbuf(stdout, NULL);


	return EXIT_SUCCESS;
}

int pedirNumero()
{
	int ingresoNumero;
	printf("ingrese un numero: ");
	scanf("%d", &ingresoNumero);

	return ingresoNumero;
}
