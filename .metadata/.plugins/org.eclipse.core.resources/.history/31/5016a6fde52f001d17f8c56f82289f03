#include <stdio.h>
#include <stdlib.h>

int restar1(int num1, int num2);


int main(void) {
	setbuf(stdout, NULL);

	int a;
	int b;

	printf("ingrese el primer numero: ");
	scanf("%d", &a);
	printf("ingrese el segundo numero: ");
	scanf("%d", &b);

	printf("la resta da: %d", restar1(a, b));

	return EXIT_SUCCESS;
}

int restar1(int num1, int num2)
{
	int resta;

	resta = num1 - num2;

	return resta;
}
