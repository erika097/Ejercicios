#include <stdio.h>
#include <stdlib.h>

int main(void) {
	setbuf(stdout, NULL);

	int numero1;
	int numero2;

	do{
		printf("ingrese el primer numero entre 10 y 100");
		scanf("%d", &numero1);
	}while(!(numero1 > 9 && numero1 < 101));

	do{
		printf("ingrese el segundo numero entre 10 y 100");
		scanf("%d", &numero2);
	}while(!(numero2 > 9 && numero2 < 101));

	return EXIT_SUCCESS;
}
