#include <stdio.h>
#include <stdlib.h>

int realizarDescuento();

int main(void) {
	setbuf(stdout, NULL);

	int numero1;

	do{
		printf("ingrese un numero entre 10 y 100");
		scanf("%d", &numero1);

	}while(!(numero1 > 9 && numero1 < 101));

	printf("se realizo un descuento con un resultado final de: %d");

	return EXIT_SUCCESS;
}

int realizarDescuento()
{
	return 0;
}
