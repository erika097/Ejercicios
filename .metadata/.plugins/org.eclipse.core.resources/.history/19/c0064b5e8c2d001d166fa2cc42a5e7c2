#include <stdio.h>
#include <stdlib.h>

int main(void) {
	setbuf(stdout, NULL);

	int num1;

	printf("ingrese un numero entre -50 y 100: ");
	scanf("%d", &num1);
	printf("ingreso: %d",num1);

	return EXIT_SUCCESS;
}
